<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".logged.autorizaciones.resultadosAutorizaciones">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/txtCrearAutoritzacio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="38dp"
            android:text="@string/resultats"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/btnReturnResultadosAutorizacion"
            android:layout_width="78dp"
            android:layout_height="86dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="100dp"
            android:contentDescription="TODO"
            android:src="@drawable/ic_baseline_salir"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtCrearAutoritzacio" />

        <ImageView
            android:id="@+id/btnResultadosAutorizacionToHome"
            android:layout_width="108dp"
            android:layout_height="68dp"
            android:layout_marginTop="148dp"
            android:layout_marginEnd="52dp"
            android:src="@drawable/ic_baseline_inicio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtCrearAutoritzacio" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>